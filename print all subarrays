public class Solution {

    public static void main(String[] args) {
        int arr[] = {1, 2, 3, 4, 5};
        int n = sc.nextInt():
        
        for(int i = 0 ; i < n ; i++){
            for (int j = i; j < n; j++){
                for(int x = i ; x <= j ; x++){
                    System.out.print(arr[x] + " ");
                }
                 System.out.println();
            }
        }
    }
}
